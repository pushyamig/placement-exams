{
    "id": "placement-exams-fixture-schema.tl.it.umich.edu",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "definitions": {
        "canvas_id": {
            "type": "integer",
            "minLength": 6,
            "maxLength": 6
        },
        "report_object": {
            "type": "object",
            "properties": {
                "id": {"type": "integer"},
                "name": {"type": "string"},
                "contact": {"type": "string"}
            },
            "required": ["id", "name", "contact"]
        },
        "exam_object": {
            "type": "object",
            "properties": {
                "sa_code": {"type": "string"},
                "name": {"type": "string"},
                "report_id": {"type": "integer"},
                "course_id": {"$ref": "#/definitions/canvas_id"},
                "assignment_id": {"$ref": "#/definitions/canvas_id"}
            },
            "required": ["sa_code", "name", "report_id", "course_id", "assignment_id"]
        }             
    },
    "properties": {
        "reports": {
            "type": "array",
            "items": {"$ref": "#/definitions/report_object"}
        },
        "exams": {
            "type": "array",
            "items": {"$ref": "#/definitions/exam_object"}
        }
    },
    "required": ["reports", "exams"]
}